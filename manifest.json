{
    "manifest_version": 3,
    "name": "LLamaParse",
    "description": "LLamas are cute",
    "version": "1.0",
    "action": {
        "default_popup": "index.html",
        "default_icon": "icon.png"
    },
    "host_permissions": ["http://*/", "https://*/"],
    "permissions": [
        "activeTab",
        "scripting",
        "tabs",
        "storage"
    ]
}